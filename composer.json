{
  "name": "doppiogancio/mocked-symfony-client",
  "description": "A simple way to mock Symfony clients",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "mock",
    "http",
    "client",
    "testing",
    "symfony",
    "php"
  ],
  "autoload": {
    "psr-4": {
      "DoppioGancio\\MockedSymfonyClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DoppioGancio\\MockedSymfonyClient\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Fabrizio Gargiulo",
      "email": "fabrizio.gargiulo@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/contracts": "^2.5",
    "psr/http-message": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^1.10",
    "symfony/var-dumper": "^7.0",
    "symfony/http-foundation": "^7.0"
  }
}
